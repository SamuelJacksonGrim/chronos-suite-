{
  "name": "chronos-suite",
  "version": "0.2.0",
  "description": "Chronos OS layer + Viz + Resonance architecture (TypeScript)",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p .",
    "dev": "ts-node-esm src/index.ts",
    "start": "node --enable-source-maps dist/index.js",
