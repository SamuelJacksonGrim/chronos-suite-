  "keywords": ["chronos", "resonance", "svg", "diagram"],
  "license": "MIT",
  "devDependencies": {
    "ts-node": "^10.9.1",
    "typescript": "^5.4.2",
    "@types/node": "^18.0.0"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
